{
    "env": {
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [2, 4],
        "strict": [2, "global"],
        "linebreak-style": [2, "unix"],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}
